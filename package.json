  "scripts": {
    "build-css": "node-sass-chokidar --include-path src src/styles/global/index.scss public/global/picstagraph-web/css/index.css --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar --watch --include-path src src/styles/global/index.scss public/global/picstagraph-web/css/index.css",
    "start-js": "react-scripts start",
    "start": "pm2 start -f ./server.sh --name picstagraph-web",
    "dev": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "clean": "rimraf {bin,build}",
    "clean-cdn": "rimraf build/global",
    "test": "react-scripts test --env=jsdom",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,scss}\"",
    "lint": "eslint src",
    "lint-fix": "yarn lint --fix",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "prepush": "yarn lint",
    "storybook": "yarn build-css && start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy": "aws s3 cp --recursive ./build s3://picstagraph.com"
  },
  "devDependencies": {
    "@angular/language-service": "^7.2.0",
    "@storybook/addon-actions": "^3.3.12",
    "@storybook/addon-info": "^3.3.12",
    "@storybook/addon-knobs": "^3.3.13",
    "@storybook/addon-links": "^3.3.12",
    "@storybook/addons": "^3.3.12",
    "@storybook/react": "^3.3.12",
    "axios-mock-adapter": "^1.10.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "9.0.0",
    "deep-freeze": "^0.0.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "5.6.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.11.1",
    "jest-localstorage-mock": "^2.1.0",
    "lint-staged": "^6.0.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "pm2": "^3.3.1",
    "prettier": "^1.9.2",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "serve": "^10.1.2",
    "storybook-router": "^0.3.2",
    "uglify-js-brunch": "^2.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintIgnore": [
    "packages/"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
